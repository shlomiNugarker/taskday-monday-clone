{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run tailwind:build && vite",
    "dev:full": "concurrently \"npm run tailwind:watch\" \"vite\"",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "tailwind:watch": "tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/tailwind.output.css --watch",
    "tailwind:build": "tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/tailwind.output.css"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@innologica/vue-dropdown-menu": "^0.1.4",
    "axios": "^0.26.1",
    "chart.js": "^3.7.1",
    "element-plus": "^2.1.11",
    "esbuild": "^0.14.28",
    "lodash": "^4.17.21",
    "sass": "^1.49.9",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "vue": "^3.2.31",
    "vue-chart-3": "^3.1.4",
    "vue-router": "^4.0.12",
    "vue-snip": "^2.0.0",
    "vue3-smooth-dnd": "^0.0.2",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/test-utils": "^2.0.0-rc.18",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "unplugin-auto-import": "^0.6.6",
    "unplugin-vue-components": "^0.18.4",
    "vite": "^2.9.18",
    "vitest": "^0.5.0"
  }
}
